{
  "name": "tiempo-compensatorio",
  "version": "0.1.9",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/Rdavid1991/tiempo-compensatorio.git"
  },
  "dependencies": {
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "13.5.0",
    "electron-updater": "4.6.5",
    "moment": "2.29.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "6.1.1",
    "react-scripts": "5.0.0",
    "sweetalert2": "11.3.0",
    "web-vitals": "2.1.2"
  },
  "homepage": "./",
  "main": "./app/index.js",
  "scripts": {
    "start-electron": "set \"ELECTRON_ENV=DEV\" && electron .",
    "build-windows": "set \"ELECTRON_ENV=PROD\" && electron-builder --publish always",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "build": {
    "directories": {
      "output": "dist"
    },
    "win": {
      "icon": "icon.*"
    },
    "files": [
      "./app/**/*",
      "*.map",
      "icon.*"
    ],
    "extraMetadata": {
      "main": "./app/index.js"
    },
    "publish": {
      "provider": "github",
      "token": "%GH_TOKEN%"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^16.0.4",
    "electron-builder": "^22.14.5",
    "electron-reloader": "^1.2.3",
    "eslint": "^8.5.0",
    "eslint-plugin-react": "^7.28.0"
  }
}